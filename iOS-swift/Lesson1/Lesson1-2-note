//控制流程
for(var i = 1 ; i <= 10 ; i+=){
	println(i) ;
}
=> for i in 1...10{
	println(i) ;
}

var studentScores = [
	"Name1":value1,
	"Name2":value2,
	"Name3":value3,
] 

for score in studentScores(Dictionary){
	if(score.1 < 60){
		println(score.0) ;
	}
}
