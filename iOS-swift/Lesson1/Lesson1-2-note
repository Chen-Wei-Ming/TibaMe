var studentGender = [
	["Name" :"Name1" ,"Sex" :"m"] ,
	["Name" :"Name2" ,"Sex" :"m"] ,
	["Name" :"Name3" ,"Sex" :"f"] ,
	["Name" :"Name4" ,"Sex" :"f"] ,
]
for person in studentGender{
	if(person["Sex"] == "f"){
		println(person["Name"]!) ;
	}
}